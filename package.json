    "axios": "^1.13.2",
    "classnames": "^2.5.1",
    "expo": "^54.0.0",
    "expo-constants": "~18.0.10",
    "expo-device": "~8.0.9",
    "expo-haptics": "~15.0.7",
    "expo-network": "~8.0.7",
    "expo-notifications": "~0.32.13",
